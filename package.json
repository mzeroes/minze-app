{
  "name": "minze",
  "version": "5.3.0",
  "private": true,
  "scripts": {
    "android": "source fastlane/.env; source fastlane/.env.secret; cd android ; ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release ; cd ..",
    "apk": "cd android ; ./gradlew assembleRelease",
    "assets": "./scripts/assets.sh",
    "compile": "tsc --noEmit -p . --pretty",
    "deploy": "./scripts/deploy.sh",
    "fastlane": "./scripts/fastlane.sh",
    "fast:install": "./scripts/fastlane.sh install_debug",
    "fast:build": "./scripts/fastlane.sh build",
    "format:js": "prettier --write {.,**}/*.js",
    "format:json": "prettier --write {.,**}/*.json",
    "format:md": "prettier --write {.,**}/*.md",
    "format:ts": "prettier --write {.,**}/*.{ts,tsx}",
    "format": "npm-run-all format:*",
    "ios": "react-native run-ios --simulator=\"iPhone 6\"",
    "lint-staged": "lint-staged",
    "lint:ts": "eslint index.js app --fix --ext .js,.ts,.tsx",
    "lint": "npm-run-all lint:*",
    "postinstall": "./scripts/post-install.sh",
    "reload": "adb shell input text \"RR\"",
    "secrets:pack": "sudo ./scripts/secrets.sh -m pack",
    "secrets:unpack": "sudo ./scripts/secrets.sh",
    "start:storybook": "react-native start",
    "start": "react-native start",
    "detox": "detox test -c android.attached.debug --reuse",
    "test": "jest -u --no-cache --coverage --verbose",
    "validate": "yarn test && yarn lint && tsc --noEmit",
    "wireless": "adb tcpip 5555; adb tcpip 192.168.0.101:5555; adb connect 192.168.0.101:5555"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shubhamxy/minze.git"
  },
  "bugs": {
    "url": "https://github.com/shubhamxy/minze/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "app/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
